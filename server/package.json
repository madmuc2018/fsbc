{
  "name": "generated-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prefix": "NODE_PATH=~/fsbc/node_modules PORT=9000",
    "yarn-install": "yarn --modules-folder ~/fsbc/node_modules install",
    "yarn-add": "yarn --modules-folder ~/fsbc/node_modules add",
    "yarn-remove": "yarn --modules-folder ~/fsbc/node_modules remove",
    "lint": "~/fsbc/node_modules/.bin/eslint --fix core/**/*.js middlewares/**/*.js tests/**/*.js bin/www",
    "production": "NODE_PATH=~/fsbc/node_modules PORT=9000 forever start -l forever.log -o out.log -e err.log ./bin/www",
    "dev": "NODE_PATH=~/fsbc/node_modules PORT=9000 ~/fsbc/node_modules/nodemon/bin/nodemon.js --ignore './config' -L ./bin/www",
    "nodb-dev": "NODE_PATH=~/fsbc/node_modules PORT=9000 NULL_DB=1 ~/fsbc/node_modules/nodemon/bin/nodemon.js --trace-warnings --ignore './config' -L ./bin/www",
    "debug-dev": "NODE_PATH=~/fsbc/node_modules PORT=9000 node inspect ./bin/www",
    "test": "NODE_PATH=~/fsbc/node_modules PORT=9000 NULL_DB=1 node ~/fsbc/node_modules/mocha/bin/mocha --trace-warnings ./tests/data.test.js --timeout 60000 --exit",
    "debug-test": "NODE_PATH=~/fsbc/node_modules PORT=9000 NULL_DB=1 node ~/fsbc/node_modules/mocha/bin/mocha debug ./tests/data.test.js",
    "coverage": "NODE_PATH=~/fsbc/node_modules PORT=9000 ./node_modules/.bin/nyc --reporter=text --reporter=lcov ./node_modules/mocha/bin/mocha ./tests/data.test.js --timeout 60000 --exit"
  },
  "dependencies": {
    "async": "^2.6.1",
    "composer-client": "^0.20.6",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-fileupload": "^1.0.0-alpha.1",
    "http-status-codes": "^1.3.0",
    "ipfs-api": "25",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.4.3",
    "morgan": "~1.9.0",
    "pify": "^4.0.0",
    "ramda": "^0.26.1",
    "random-item": "^1.0.0",
    "shelljs": "^0.8.3",
    "superagent": "^4.0.0-beta.5",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.9",
    "supertest": "^3.3.0"
  }
}
