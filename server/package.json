{
  "name": "generated-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "yarn-install": "yarn --modules-folder ~/fsbc/node_modules install",
    "yarn-add": "yarn --modules-folder ~/fsbc/node_modules add",
    "yarn-remove": "yarn --modules-folder ~/fsbc/node_modules remove",
    "lint": "~/fsbc/node_modules/.bin/eslint --fix core/**/*.js middlewares/**/*.js tests/**/*.js bin/www",
    "production": "NODE_PATH=~/fsbc/node_modules forever start --append -l ~/fsbc/log/forever.log -o ~/fsbc/log/out.log -e ~/fsbc/log/err.log ./bin/www --color",
    "dev": "NODE_PATH=~/fsbc/node_modules ~/fsbc/node_modules/nodemon/bin/nodemon.js --ignore './config' -L ./bin/www",
    "debug-dev": "NODE_PATH=~/fsbc/node_modules node inspect ./bin/www",
    "test": "LOCAL_DB=true NODE_PATH=~/fsbc/node_modules node ~/fsbc/node_modules/mocha/bin/mocha --trace-warnings ./tests/data.test.js --timeout 60000 --exit",
    "test-dev": "LOCAL_DB=true NODE_PATH=~/fsbc/node_modules node ~/fsbc/node_modules/mocha/bin/mocha ./tests/data.test.js --grep devtest --timeout 60000 --exit",
    "debug-test": "LOCAL_DB=true NODE_PATH=~/fsbc/node_modules node ~/fsbc/node_modules/mocha/bin/mocha debug ./tests/data.test.js",
    "debug-test-dev": "LOCAL_DB=true NODE_PATH=~/fsbc/node_modules node ~/fsbc/node_modules/mocha/bin/mocha debug ./tests/data.test.js --grep devtest --timeout 60000 --exit",
    "coverage": "NODE_PATH=~/fsbc/node_modules ./node_modules/.bin/nyc --reporter=text --reporter=lcov ./node_modules/mocha/bin/mocha ./tests/data.test.js --timeout 60000 --exit",
    "backup-worker": "NODE_PATH=~/fsbc/node_modules forever start --append -l ~/fsbc/log/backupForever.log -o ~/fsbc/log/backupOut.log -e ~/fsbc/log/backupErr.log core/storage/backupWorker.js --color",
    "debug-backup-worker": "NODE_PATH=~/fsbc/node_modules node inspect core/storage/backupWorker.js",
    "repl": "NODE_PATH=~/fsbc/node_modules node"
  },
  "dependencies": {
    "async": "^2.6.1",
    "chalk": "^2.4.2",
    "composer-client": "^0.20.6",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-fileupload": "^1.0.0-alpha.1",
    "http-status-codes": "^1.3.0",
    "ipfs-api": "25",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.3",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "~1.9.0",
    "object-hash": "^1.3.1",
    "pify": "^4.0.0",
    "random-item": "^1.0.0",
    "shelljs": "^0.8.3",
    "superagent": "^4.0.0-beta.5",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.9",
    "supertest": "^3.3.0"
  }
}
