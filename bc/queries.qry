query getData {
  description: "Get Data asset by GUID"
  statement: 
    SELECT org.dfs.Data
    WHERE (guid == _$guid AND owner == _$username)
}

query getNewerVersionOfData {
  description: "Get Data asset whose lastVersion points to the provided GUID"
  statement: 
    SELECT org.dfs.Data
    WHERE (lastVersion == _$guid AND owner == _$username)
}

query getUser {
  description: "Get User by username"
  statement: 
    SELECT org.dfs.User
    WHERE (username == _$username)
}